{
    "and": [
        {
            "property": "Missing_Nest",
            "checkbox": {
                "equals": true
            }
        },
        {
            "property": "Job status",
            "select": {
                "equals": "Nest"
            }
        },
        {
            "property": "Order status",
            "rollup": {
                "every": {
                    "select": {
                        "does_not_equal": "Canceled"
                    }
                }
            }
        },
        {
            "property": "Order status",
            "rollup": {
                "every": {
                    "select": {
                        "does_not_equal": "Invoiced"
                    }
                }
            }
        },
        {
            "property": "Order status",
            "rollup": {
                "every": {
                    "select": {
                        "does_not_equal": "Shipped"
                    }
                }
            }
        },
        {
            "property": "System status",
            "select": {
                "equals": "Active"
            }
        }
    ]
}